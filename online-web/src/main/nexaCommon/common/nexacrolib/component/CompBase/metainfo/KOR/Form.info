        <Return type="Object" description="[XComp 기준 팝업위치(&quot;left&quot;, &quot;top&quot;, &quot;right&quot;, &quot;bottom&quot;), x, y [ ,width, height] ]"/>
        <Arguments>
          <Argument name="XComp" type="Object" description="기준이 되는 nexacro Component. "/>
          <Argument name="width" type="Number" description="표시할 팝업 width."/>
          <Argument name="height" type="Number" description="표시할 팝업 height."/>
          <Argument name="direction" type="Object" description="팝업표시 방향.&quot;vert&quot;: vertical(default),&quot;horz&quot;: horizontal."/>
          <Argument name="offset" type="Object" description="표시될때 XComp와의 간격(default: 0)."/>
        </Arguments>
      </Syntax>
    </Method>
	
